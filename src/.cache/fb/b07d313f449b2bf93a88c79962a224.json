: flex;\n\t\tjustify-content: space-around;\n\t\talign-items: center;\n\t\ttext-align: center;\n\t\tz-index: 12;\n\t}\n\t\n\t\n\t\n\t.prompt-hor {\n\t\tdisplay: none;\n\t\tcolor:black;\n\t}\n\t\n\t@media screen and (orientation: portrait) {\n\t\thtml {\n\t\t}\n\t\t\n\t\t.prompt-hor {\n\t\t\tfont-size: 2em;\n\t\t\tz-index: 500;  \n\t\t\theight:100% !important;\n\t\t\twidth:100%;\n\t\t\tposition: fixed;\n\t\t\ttext-align: center;\n\t\t\talign-items: center;\n\t\t\tjustify-content: center;\n\t\t\ttop:0%;\n\t\t\tleft:0%;\n\t\t\tbackground-color: black;\n\t\t\t// border:5px solid orange;\n\t\t\tjustify-content:center ;\n\t\t\tbackground-image: url('../../img/g90.png');\n\t\t\tdisplay: grid; \n\t\t\tbackground-repeat:no-repeat;\n\t\t\tbackground-size: cover;\n\t\t}\n\t\t.turn, .turn2{\n\t\t\t// border:4px solid blue;\n\t\t\tz-index: 501;\n\t\t\ttransform:translateY(250px) ;\n\t\t\twidth: 500px !important;\n\t\t\theight: 500px;\n\t\t\tbackground-image:url('../../img/turn.png');\n\t\t\tbackground-repeat: no-repeat;\n\t\t\tdisplay: grid;\n\t\t\t\n\t\t}\n\t\t.turn2{\n\t\t\ttransform: rotate(90deg);\n\t\t}\n\t}\n\t.hidden{display:none}"},"lineCount":null}},"error":null,"hash":"df1542f1f0c191bf2495c8ec2ba8bb58","cacheData":{"env":{}}}